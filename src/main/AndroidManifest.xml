<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tenghu.activity" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity" android:label="@string/main_home_str"/>
        <activity android:name=".ZegapainActivity" android:label="@string/main_zegapain_str" android:windowSoftInputMode="stateAlwaysHidden"/>
        <activity android:name=".ToolsActivity" android:label="@string/main_tools_str"/>
    </application>

</manifest>
